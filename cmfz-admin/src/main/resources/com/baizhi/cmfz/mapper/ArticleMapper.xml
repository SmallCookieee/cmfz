<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "../mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.cmfz.dao.ArticleDao">
	
    <insert id="insertArticle">
        insert into t_article set article_id=#{articleId},
            article_name=#{articleName},introduction=#{introduction},
            article_date=#{articleDate},article_src=#{articleSrc},
            article_author=#{articleAuthor},article_status=#{articleStatus}
    </insert>

</mapper>
